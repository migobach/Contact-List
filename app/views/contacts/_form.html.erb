<%= form_for @contact do |f| %>
  
  <%= f.label :first_name %>
  <%= f.text_area :first_name %>

  <%= f.label :last_name %>
  <%= f.text_area :last_name %>

  <%= f.label :phone %>
  <%= f.number_field :phone %>

  <%= f.label :email %>
  <%= f.text_area :email %>

  <%= f.label :address %>
  <%= f.text_area :address %>

  <%= f.label :city %>
  <%= f.text_area :city %>

  <%= f.label :state %>
  <%= f.text_area :state %>

  <%= f.label :zip %>
  <%= f.number_field :zip %>

  <%= f.submit %>

<% end %>